<%@ page import="java.util.List" %>
<%@ page import="com.google.appengine.api.datastore.Entity" %>
<html>
<body>
	<h1>Torrent ITEM List</h1>

	Function : <a href="add">Add Torrent Item</a>
	<hr />

	<h2>All Torrent Items</h2>
	<table border="1">
		<thead>
			<tr>
				<td>Item</td>
				<td>Date</td>
				<td>Created Date</td>
				<td>Action</td>
			</tr>
		</thead>
		<%
		
			List<Entity> torrentItem = (List<Entity>)request.getAttribute("torrentItem");
		    for(Entity e : torrentItem){
		     
		%>
			<tr>
				<td><%=e.getProperty("item") %></td>
				<td><%=e.getProperty("date") %></td>
				<td><a href="update/<%=e.getProperty("item")%>">Update</a> | <a href="delete/<%=e.getProperty("item")%>">Delete</a></td>
			</tr>
		<%
			}
		%>
	</table>

</body>
</html>